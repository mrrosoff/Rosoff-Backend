{
	"name": "kings-corner",
	"author": "Meta Games",
	"version": "2.0.0",
	"description": "A Modern Chess Web App",
	"repository": "https://github.com/M3tanym/Kings-Corner",
	"license": "MIT",
	"main": "client/main.cjs",
	"scripts": {
		"test:unit": "mocha --require @babel/register --recursive",
		"test:coverage": "c8 --all --check-coverage --lines 25 --include 'server/' npm run test:unit",
		"test": "npm run test:coverage",
		"server": "webpack --watch --config server/webpack.server.config.mjs",
		"mockServer": "MOCK=true npm run server",
		"startWebsite": "webpack serve --config client/webpack.config.mjs",
		"start": "concurrently --kill-others-on-fail \"npm:server\" \"npm:startWebsite\"",
		"buildServer": "webpack --config server/webpack.server.config.mjs",
		"buildWebsite": "webpack --config client/webpack.config.mjs",
		"build": "concurrently --kill-others-on-fail \"npm:buildServer\" \"npm:buildWebsite\""
	},
	"dependencies": {
		"@apollo/client": "^3.3.19",
		"@babel/plugin-transform-runtime": "^7.14.3",
