		"framer-motion": "^4.1.17",
		"google-auth-library": "^7.1.0",
		"graphql": "^15.5.0",
		"graphql-voyager": "*",
		"handlebars": "^4.7.7",
		"https": "1.0.0",
		"jsonwebtoken": "^8.5.1",
		"moment": "^2.29.1",
		"mongodb": "^3.6.8",
		"nodemailer": "^6.6.1",
		"notistack": "^1.0.9",
		"react": "^17.0.2",
		"react-apple-login": "^1.1.3",
		"react-dnd": "^14.0.2",
