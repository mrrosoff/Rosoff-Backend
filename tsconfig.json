{
	"compilerOptions": {
		"allowJs": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"lib": ["DOM", "ESNext"],
		"module": "ESNext",
		"sourceMap": true,
		"target": "ESNext",
		"strict": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"moduleResolution": "node",
		"resolveJsonModule": true
	},
	"include": ["client"],
	"exclude": ["**/*.d.ts"]
}
